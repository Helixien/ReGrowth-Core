	<ThingDef Name="RG_BaseFilthLeaves" ParentName="BaseFilth" Abstract="True">
		<hideAtSnowDepth>0.3</hideAtSnowDepth>
		<filth>
			<disappearsInDays>45~50</disappearsInDays>
			<cleaningWorkToReduceThickness>35</cleaningWorkToReduceThickness>
			<rainWashes>false</rainWashes>
		</filth>
		<statBases>
			<Beauty>-8</Beauty>
			<BeautyOutdoors>0</BeautyOutdoors>
		</statBases>
	</ThingDef>
	<!-- ================ Leaf Filth ================ -->
	<ThingDef ParentName="RG_BaseFilthLeaves">
		<defName>RG_Filth_LeavesTreeOak</defName>
		<label>leaves</label>
		<graphicData>
			<texPath>Things/Filth/LeafShapeOne</texPath>
		</graphicData>
	</ThingDef>
	<ThingDef ParentName="RG_BaseFilthLeaves">
		<defName>RG_Filth_LeavesTreePopolar</defName>
		<label>leaves</label>
		<graphicData>
			<texPath>Things/Filth/LeafShapeTwo</texPath>
		</graphicData>
	</ThingDef>
	<ThingDef ParentName="RG_BaseFilthLeaves">
		<defName>RG_Filth_LeavesTreeBirch</defName>
		<label>leaves</label>
		<graphicData>
			<texPath>Things/Filth/LeafShapeThree</texPath>
		</graphicData>
	</ThingDef>
	<!-- ================ Neutroamine Filth ================ -->
	<ThingDef ParentName="BaseFilth">
		<defName>RG_Filth_Neutroamine</defName>
		<label>neutroamine</label>
		<statBases>
			<Beauty>-30</Beauty>
			<Cleanliness>-10</Cleanliness>
		</statBases>
		<graphicData>
			<texPath>Things/Filth/Spatter</texPath>
			<color>(94, 159, 166)</color>
		</graphicData>
		<filth>
			<ignoreFilthMultiplierStat>true</ignoreFilthMultiplierStat>
			<disappearsInDays>35~40</disappearsInDays>
			<rainWashes>true</rainWashes>
			<cleaningWorkToReduceThickness>70</cleaningWorkToReduceThickness>
			<canFilthAttach>true</canFilthAttach>
			<cleaningSound>Interact_CleanFilth_Fluid</cleaningSound>
		</filth>
	</ThingDef>
