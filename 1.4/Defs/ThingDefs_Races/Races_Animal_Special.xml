	<!--====================  Neutrolope ===========================-->
	<ThingDef ParentName="AnimalThingBase">
		<defName>RG_Neutrolope</defName>
		<label>neutrolope</label>
		<devNote>cow-1</devNote>
		<description>Engineered for neutroamine production, the neutrolope grows a large sac of pure neutroamine on its back. Though it is weak and fragile for its size, other animals avoid it similarly to a boomalope, despite the fact it does not explode when it dies. It can be milked to produce neutroamine.</description>
		<statBases>
			<MoveSpeed>3.4</MoveSpeed>
			<MarketValue>480</MarketValue>
			<ComfyTemperatureMin>-15</ComfyTemperatureMin>
			<FilthRate>16</FilthRate>
			<ToxicEnvironmentResistance MayRequire="Ludeon.RimWorld.Biotech">0.9</ToxicEnvironmentResistance>
		</statBases>
		<race>
			<body>QuadrupedAnimalWithHoovesAndHump</body>
			<deathActionWorkerClass>ReGrowthCore.DeathActionWorker_Neutrolope</deathActionWorkerClass>
			<executionRange>6</executionRange>
			<herdAnimal>true</herdAnimal>
			<baseBodySize>2.0</baseBodySize>
			<baseHungerRate>0.535</baseHungerRate>
			<baseHealthScale>0.65</baseHealthScale>
			<foodType>VegetarianRoughAnimal</foodType>
			<leatherDef>Leather_Plain</leatherDef>
			<wildness>0.6</wildness>
			<roamMtbDays>2</roamMtbDays>
			<canBePredatorPrey>false</canBePredatorPrey>
			<gestationPeriodDays>6.66</gestationPeriodDays>
			<trainability>None</trainability>
			<manhunterOnDamageChance>0.1</manhunterOnDamageChance>
			<manhunterOnTameFailChance>0.1</manhunterOnTameFailChance>
			<lifeExpectancy>15</lifeExpectancy>
			<wildBiomes>
				<TemperateForest>0.01</TemperateForest>
				<RG_TemperateGrassland>0.01</RG_TemperateGrassland>
				<TemperateSwamp>0.01</TemperateSwamp>
				<TropicalRainforest>0.01</TropicalRainforest>
				<TropicalSwamp>0.01</TropicalSwamp>
				<AridShrubland>0.01</AridShrubland>
				<Desert>0.01</Desert>
				<RG_BoilingForest MayRequire="ReGrowth.BOTR.BoilingForest">0.01</RG_BoilingForest>
			</wildBiomes>
			<lifeStageAges>
				<li>
					<def>AnimalBaby</def>
					<minAge>0</minAge>
				</li>
				<li>
					<def>AnimalJuvenile</def>
					<minAge>0.2</minAge>
				</li>
				<li>
					<def>AnimalAdult</def>
					<minAge>0.3333</minAge>
					<soundWounded>Pawn_Muffalo_Wounded</soundWounded>
					<soundDeath>Pawn_Muffalo_Death</soundDeath>
					<soundCall>Pawn_Muffalo_Call</soundCall>
					<soundAngry>Pawn_Muffalo_Angry</soundAngry>
				</li>
			</lifeStageAges>
			<soundMeleeHitPawn>Pawn_Melee_BigBash_HitPawn</soundMeleeHitPawn>
			<soundMeleeHitBuilding>Pawn_Melee_BigBash_HitBuilding</soundMeleeHitBuilding>
			<soundMeleeMiss>Pawn_Melee_BigBash_Miss</soundMeleeMiss>
			<headPosPerRotation>
				<li>(0.0, 0, 0.22)</li>
				<li>(0.38, 0, 0.01)</li>
				<li>(0.0, 0, -0.165)</li>
				<li>(-0.38, 0, 0.01)</li>
			</headPosPerRotation>
		</race>
		<tradeTags>
			<li>AnimalUncommon</li>
			<li>AnimalExotic</li>
		</tradeTags>
		<tools>
			<li>
				<label>head</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>7</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
				<ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
				<chanceFactor>0.2</chanceFactor>
			</li>
			<li>
				<label>left hoof</label>
				<capacities>
					<li>Blunt</li>
					<li>Poke</li>
				</capacities>
				<power>9</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>FrontLeftLeg</linkedBodyPartsGroup>
			</li>
			<li>
				<label>right hoof</label>
				<capacities>
					<li>Blunt</li>
					<li>Poke</li>
				</capacities>
				<power>9</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>FrontRightLeg</linkedBodyPartsGroup>
			</li>
			<li>
				<capacities>
					<li>Bite</li>
				</capacities>
				<power>10</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
				<chanceFactor>0.5</chanceFactor>
			</li>
		</tools>
		<comps>
			<li Class="CompProperties_Milkable">
				<milkDef>Neutroamine</milkDef>
				<milkIntervalDays>5</milkIntervalDays>
				<milkAmount>15</milkAmount>
				<milkFemaleOnly>false</milkFemaleOnly>
			</li>
		</comps>
	</ThingDef>
