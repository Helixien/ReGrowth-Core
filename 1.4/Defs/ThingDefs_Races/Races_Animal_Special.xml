<Defs>
	<!--====================  Moa ===========================-->
	<ThingDef ParentName="BigBirdThingBase">
		<defName>RG_Moa</defName>
		<label>moa</label>
		<description>The largest and most commonly found modified bird species, moa are known for their very lizard-like heads, delicious meat, and powerful kick. It is the result of genetic engineering, however the details of its creation have been lost to time. It is skittish and will flee when attacked.</description>
		<statBases>
			<MoveSpeed>4.5</MoveSpeed>
			<MarketValue>300</MarketValue>
			<ComfyTemperatureMin>-8</ComfyTemperatureMin>
			<ComfyTemperatureMax>50</ComfyTemperatureMax>
			<FilthRate>8</FilthRate>
			<ToxicEnvironmentResistance MayRequire="Ludeon.RimWorld.Biotech">0.9</ToxicEnvironmentResistance>
		</statBases>
		<race>
			<herdAnimal>true</herdAnimal>
			<baseBodySize>0.6</baseBodySize>
			<baseHealthScale>1</baseHealthScale>
			<baseHungerRate>0.5</baseHungerRate>
			<foodType>VegetarianRoughAnimal</foodType>
			<wildness>0.95</wildness>
			<lifeExpectancy>45</lifeExpectancy>
			<meatLabel>moa meat</meatLabel>
			<manhunterOnTameFailChance>0.05</manhunterOnTameFailChance>
			<manhunterOnDamageChance>0.1</manhunterOnDamageChance>
			<lifeStageAges>
				<li>
					<def>AnimalBabyTiny</def>
					<minAge>0</minAge>
					<soundWounded>Pawn_Chick_Call</soundWounded>
					<soundDeath>Pawn_Chick_Call</soundDeath>
					<soundCall>Pawn_Chick_Call</soundCall>
					<soundAngry>Pawn_Chick_Call</soundAngry>
				</li>
				<li>
					<def>AnimalJuvenile</def>
					<minAge>0.1</minAge>
					<soundWounded>RG_Pawn_Moa_Wounded</soundWounded>
					<soundDeath>RG_Pawn_Moa_Death</soundDeath>
					<soundCall>RG_Pawn_Moa_Call</soundCall>
					<soundAngry>RG_Pawn_Moa_Angry</soundAngry>
				</li>
				<li>
					<def>AnimalAdult</def>
					<minAge>0.2222</minAge>
					<soundWounded>RG_Pawn_Moa_Wounded</soundWounded>
					<soundDeath>RG_Pawn_Moa_Death</soundDeath>
					<soundCall>RG_Pawn_Moa_Call</soundCall>
					<soundAngry>RG_Pawn_Moa_Angry</soundAngry>
				</li>
			</lifeStageAges>
		</race>
		<tools>
			<li>
				<label>claws</label>
				<capacities>
					<li>Scratch</li>
				</capacities>
				<power>7</power>
				<cooldownTime>1.5</cooldownTime>
				<linkedBodyPartsGroup>Feet</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>14</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>beak</label>
				<capacities>
					<li>Bite</li>
				</capacities>
				<power>12</power>
				<cooldownTime>1.6</cooldownTime>
				<linkedBodyPartsGroup>Beak</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>12</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>head</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>5</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
				<ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
				<chanceFactor>0.5</chanceFactor>
			</li>
		</tools>
		<comps>
			<li Class="CompProperties_EggLayer">
				<eggFertilizedDef>RG_EggMoaFertilized</eggFertilizedDef>
				<eggFertilizationCountMax>1</eggFertilizationCountMax>
				<eggLayIntervalDays>4.44</eggLayIntervalDays>
				<eggProgressUnfertilizedMax>0.5</eggProgressUnfertilizedMax>
				<eggCountRange>
					<min>1</min>
					<max>3</max>
				</eggCountRange>
			</li>
		</comps>
		<tradeTags>
			<li>AnimalExotic</li>
			<li>AnimalFarm</li>
		</tradeTags>
	</ThingDef>
