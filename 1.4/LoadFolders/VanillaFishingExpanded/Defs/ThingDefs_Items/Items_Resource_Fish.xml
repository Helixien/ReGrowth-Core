<Defs>
	<!--=============== Hot Spring Fish ==============-->
	<ThingDef ParentName="VCEF_RawFishBase">
		<defName>RG_RawDevilsHole</defName>
		<label>devils hole pupfish</label>
		<description>Raw devils hole pupfish. A small, purple fish belonging to the Cyprinodontidae family. It can only be found in hot spring waters and is extremely rare.</description>
		<graphicData>
			<texPath>Things/Item/Resource/DevilsHole</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<Mass>0.05</Mass>
			<Nutrition>0.05</Nutrition>
			<MarketValue>0.55</MarketValue>
		</statBases>
		<ingestible>
			<specialThoughtDirect>AteRawFood</specialThoughtDirect>
		</ingestible>
		<tools>
			<li>
				<label>devils hole pupfish</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>1.5</power>
				<cooldownTime>2</cooldownTime>
			</li>
		</tools>
		<equippedAngleOffset>-180</equippedAngleOffset>
		<uiIconForStackCount>1</uiIconForStackCount>
		<equipmentType>Primary</equipmentType>
		<techLevel>Neolithic</techLevel>
		<comps>
			<li>
				<compClass>CompEquippable</compClass>
			</li>
		</comps>
	</ThingDef>
