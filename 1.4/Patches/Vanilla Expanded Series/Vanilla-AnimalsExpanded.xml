<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<!--============================================== Vanilla Animals Expanded ==============================================-->
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Animals Expanded</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/BiomeDef[defName="RG_TemperateGrassland"]/wildAnimals</xpath>
					<value>
						<AEXP_RedPanda>0.3</AEXP_RedPanda>
						<AEXP_Badger>0.1</AEXP_Badger>
						<AEXP_WildGoose>0.25</AEXP_WildGoose>
						<AEXP_Beaver>0.1</AEXP_Beaver>
						<AEXP_Hedgehog>0.5</AEXP_Hedgehog>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
	<!--============================================== Vanilla Animals Expanded - Waste Animals ==============================================-->
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Animals Expanded â€” Waste Animals</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/BiomeDef[defName="RG_TemperateGrassland"]/pollutionWildAnimals</xpath>
					<value>
						<VAEWaste_Toxafox>0.035</VAEWaste_Toxafox>
						<VAEWaste_Wasteboar>0.25</VAEWaste_Wasteboar>
						<VAEWaste_Wastedeer>0.25</VAEWaste_Wastedeer>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>