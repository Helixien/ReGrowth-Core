<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<!--============================================== Vanilla Fishing Expanded ==============================================-->
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Fishing Expanded</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/VCE_Fishing.FishableTerrainDef[defName="VCEF_VanillaFishableTerrainsFresh"]/allowedTerrains</xpath>
					<value>
						<li>RG_HotSpring</li>
						<li>RG_HotSpringDeep</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/VCE_Fishing.BiomeTempDef[defName="VCEF_BiomeWarm"]/biomes</xpath>
					<value>
						<li>RG_TemperateGrassland</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>