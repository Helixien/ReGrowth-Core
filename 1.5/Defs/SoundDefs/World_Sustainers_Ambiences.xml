						<paramUpdateMode>Constant</paramUpdateMode>
						<curve>
							<points>
								<li>
									<loc>
										<x>1</x>
										<y>1</y>
									</loc>
								</li>
								<li>
									<loc>
										<x>33.34967</x>
										<y>0.9631265</y>
									</loc>
								</li>
								<li>
									<loc>
										<x>69.43261</x>
