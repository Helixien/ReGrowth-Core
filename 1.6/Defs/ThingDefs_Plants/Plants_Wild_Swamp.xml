<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<ThingDef ParentName="PlantBase">
		<defName>RG_Plant_CreepStern</defName>
		<label>creep stern</label>
		<description>Wild Ivy with star shaped leaves that grows in patches in cold and wet biomes. Takes a long time to grow, but is very frost-resistant.</description>
		<statBases>
			<MaxHitPoints>120</MaxHitPoints>
			<Flammability>0.6</Flammability>
			<Nutrition>0.15</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/RG_CreepStern</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<hideAtSnowDepth>0.5</hideAtSnowDepth>
		<ingestible/>
		<plant>
			<fertilityMin>0.05</fertilityMin>
			<fertilitySensitivity>0.2</fertilitySensitivity>
			<growDays>15</growDays>
			<leaflessGraphicPath>Things/Plant/_Leafless/RG_CreepStern_Leafless</leaflessGraphicPath>
			<pollutedGraphicPath>Things/Plant/_Polluted/RG_CreepStern_Polluted</pollutedGraphicPath>
			<harvestWork>75</harvestWork>
			<maxMeshCount>4</maxMeshCount>
			<wildClusterRadius>4</wildClusterRadius>
			<wildClusterWeight>8</wildClusterWeight>
			<visualSizeRange>0.8~1.3</visualSizeRange>
			<topWindExposure>0.25</topWindExposure>
			<wildOrder>1</wildOrder>
		</plant>
	</ThingDef>
	<ThingDef ParentName="PlantBase">
		<defName>RG_Plant_Dervish</defName>
		<label>dervish</label>
		<description>A beautiful bluish flower growing in colder and wet biomes. Can only be found in cold bogs.</description>
		<graphicData>
			<texPath>Things/Plant/RG_Dervish</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<statBases>
			<MaxHitPoints>120</MaxHitPoints>
			<Beauty>6</Beauty>
			<Nutrition>0.25</Nutrition>
		</statBases>
		<selectable>true</selectable>
		<neverMultiSelect>false</neverMultiSelect>
		<pathCost>5</pathCost>
		<ingestible/>
		<plant>
			<fertilityMin>0.05</fertilityMin>
			<fertilitySensitivity>0</fertilitySensitivity>
			<growDays>2.5</growDays>
			<mustBeWildToSow>true</mustBeWildToSow>
			<sowResearchPrerequisites/>
			<sowTags>
				<li>Ground</li>
			</sowTags>
			<maxMeshCount>16</maxMeshCount>
			<visualSizeRange>0.3~0.5</visualSizeRange>
			<topWindExposure>0.3</topWindExposure>
			<wildClusterRadius>4</wildClusterRadius>
			<wildClusterWeight>3</wildClusterWeight>
			<wildOrder>6</wildOrder>
			<purpose>Beauty</purpose>
		</plant>
	</ThingDef>
	<ThingDef ParentName="PlantBase">
		<defName>RG_Plant_SwampPod</defName>
		<label>swamp pod</label>
		<description>A small mushroom like plant that looks similar to an eye with a hard shell. It can only be found in swamps.</description>
		<graphicData>
			<texPath>Things/Plant/RG_SwampPod</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<Beauty>-2</Beauty>
			<BeautyOutdoors>0</BeautyOutdoors>
		</statBases>
		<selectable>false</selectable>
		<neverMultiSelect>false</neverMultiSelect>
		<pathCost>5</pathCost>
		<ingestible>
			<preferability>NeverForNutrition</preferability>
		</ingestible>
		<plant>
			<fertilityMin>0.04</fertilityMin>
			<fertilitySensitivity>0.15</fertilitySensitivity>
			<growDays>18</growDays>
			<maxMeshCount>9</maxMeshCount>
			<wildClusterRadius>4</wildClusterRadius>
			<wildClusterWeight>3</wildClusterWeight>
			<visualSizeRange>0.25~0.45</visualSizeRange>
			<topWindExposure>0</topWindExposure>
		</plant>
	</ThingDef>
	<ThingDef ParentName="PlantBase">
		<defName>RG_SychiCap</defName>
		<label>Sychi cap</label>
		<description>A small redish mushroom growing in clusters. It grows exclusively in cold bogs.</description>
		<graphicData>
			<texPath>Things/Plant/RG_SychiCap</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<statBases>
			<MaxHitPoints>120</MaxHitPoints>
			<Nutrition>0.3</Nutrition>
			<Beauty>4</Beauty>
			<BeautyOutdoors>4</BeautyOutdoors>
		</statBases>
		<selectable>true</selectable>
		<neverMultiSelect>false</neverMultiSelect>
		<pathCost>15</pathCost>
		<ingestible/>
		<plant>
			<fertilityMin>0.04</fertilityMin>
			<fertilitySensitivity>0.15</fertilitySensitivity>
			<growDays>8</growDays>
			<maxMeshCount>4</maxMeshCount>
			<dieIfLeafless>true</dieIfLeafless>
			<harvestYield>12</harvestYield>
			<harvestTag>Standard</harvestTag>
			<harvestedThingDef>RawFungus</harvestedThingDef>
			<neverBlightable>true</neverBlightable>
			<visualSizeRange>0.3~0.5</visualSizeRange>
			<topWindExposure>0.1</topWindExposure>
			<purpose>Food</purpose>
		</plant>
	</ThingDef>
	<ThingDef ParentName="DeciduousTreeBase">
		<defName>RG_Plant_TreeWhiteWillow</defName>
		<label>willow tree</label>
		<description>A genetically modified version of the common willow. Provides a beautiful, leafy canopy in white. It doesn't yield much usable wood.</description>
		<graphicData>
			<texPath>Things/Plant/RG_WhiteWillow</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<shadowData>
				<volume>(0.2, 0.3, 0.13)</volume>
				<offset>(0,0,-0.44)</offset>
			</shadowData>
			<shaderParameters Inherit="False"/>
		</graphicData>
		<statBases>
			<Beauty>2</Beauty>
			<BeautyOutdoors>2</BeautyOutdoors>
		</statBases>
		<plant>
			<growDays>13</growDays>
			<leaflessGraphicPath>Things/Plant/_Leafless/RG_TreeWillow_Leafless</leaflessGraphicPath>
			<harvestWork>800</harvestWork>
			<harvestYield>27</harvestYield>
			<wildClusterRadius>17</wildClusterRadius>
			<wildClusterWeight>25</wildClusterWeight>
			<visualSizeRange>1.1~1.8</visualSizeRange>
		</plant>
	</ThingDef>
</Defs>