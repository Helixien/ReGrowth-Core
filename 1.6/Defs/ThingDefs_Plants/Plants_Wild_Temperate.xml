<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="PlantBase">
		<defName>RG_Plant_Raspberry</defName>
		<label>raspberry bush</label>
		<description>A bushy wild plant which yields delicious red berries. Berries can be cooked, but they're also good to eat even when raw.</description>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<Nutrition>0.4</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/RG_RaspberryPlant</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<shadowData>
				<volume>(0.55, 0.2, 0.2)</volume>
				<offset>(0,0,-0.25)</offset>
			</shadowData>
		</graphicData>
		<selectable>true</selectable>
		<neverMultiSelect>false</neverMultiSelect>
		<fillPercent>0.2</fillPercent>
		<pathCost>25</pathCost>
		<plant>
			<fertilityMin>0.4</fertilityMin>
			<fertilitySensitivity>0.4</fertilitySensitivity>
			<growDays>4</growDays>
			<leaflessGraphicPath>Things/Plant/_Leafless/RG_RaspberryPlant_Leafless</leaflessGraphicPath>
			<immatureGraphicPath>Things/Plant/_Immature/RG_RaspberryPlant_Immature</immatureGraphicPath>
			<harvestYield>7</harvestYield>
			<harvestWork>200</harvestWork>
			<harvestTag>Standard</harvestTag>
			<harvestAfterGrowth>0.3</harvestAfterGrowth>
			<harvestedThingDef>RawBerries</harvestedThingDef>
			<wildClusterRadius>5</wildClusterRadius>
			<wildClusterWeight>250</wildClusterWeight>
			<visualSizeRange>0.7~1.2</visualSizeRange>
			<topWindExposure>0.25</topWindExposure>
			<wildOrder>2</wildOrder>
			<purpose>Food</purpose>
		</plant>
	</ThingDef>
	<ThingDef ParentName="PlantBase">
		<defName>RG_Plant_TemperateIvy</defName>
		<label>ivy</label>
		<description>Wild ivy with leafs that grows in patches of leaves in areas of warm temperatures. Takes a long time to grow, but is very frost-resistant.</description>
		<statBases>
			<MaxHitPoints>120</MaxHitPoints>
			<Flammability>0.6</Flammability>
			<Nutrition>0.15</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/RG_TemperateIvy</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<hideAtSnowDepth>0.5</hideAtSnowDepth>
		<ingestible/>
		<plant>
			<fertilityMin>0.05</fertilityMin>
			<fertilitySensitivity>0.2</fertilitySensitivity>
			<growDays>15</growDays>
			<leaflessGraphicPath>Things/Plant/_Leafless/RG_TemperateIvy_Leafless</leaflessGraphicPath>
			<pollutedGraphicPath>Things/Plant/_Polluted/RG_TemperateIvy_Polluted</pollutedGraphicPath>
			<harvestWork>75</harvestWork>
			<maxMeshCount>4</maxMeshCount>
			<wildClusterRadius>4</wildClusterRadius>
			<wildClusterWeight>8</wildClusterWeight>
			<visualSizeRange>0.8~1.3</visualSizeRange>
			<topWindExposure>0.25</topWindExposure>
			<wildOrder>1</wildOrder>
		</plant>
	</ThingDef>
	<ThingDef ParentName="BushBase">
		<defName>RG_Plant_TemperateFern</defName>
		<label>fern</label>
		<description>A short shrub with long, fine leaves. Grows in warm forests areas</description>
		<graphicData>
			<texPath>Things/Plant/RG_TemperateFern</texPath>
			<drawSize>1.15</drawSize>
			<shadowData>
				<volume>(0.55, 0.2, 0.2)</volume>
			</shadowData>
		</graphicData>
		<uiIconScale>0.85</uiIconScale>
		<selectable>false</selectable>
		<plant>
			<growDays>3</growDays>
			<leaflessGraphicPath>Things/Plant/Leafless/RG_TemperateFern_Leafless</leaflessGraphicPath>
			<harvestWork>60</harvestWork>
		</plant>
	</ThingDef>
	<ThingDef ParentName="PlantBase">
		<defName>RG_Plant_BlueDandelion</defName>
		<label>dandelions</label>
		<description>A tiny blue flower which grows in large clusters. Though it is often considered a weed, dandelions in bloom are quite beautiful.</description>
		<statBases>
			<MaxHitPoints>85</MaxHitPoints>
			<Nutrition>0.18</Nutrition>
			<Beauty>4</Beauty>
			<BeautyOutdoors>4</BeautyOutdoors>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/RG_BlueDandelion</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<hideAtSnowDepth>0.5</hideAtSnowDepth>
		<ingestible/>
		<plant>
			<fertilitySensitivity>0.0</fertilitySensitivity>
			<growDays>2.5</growDays>
			<mustBeWildToSow>true</mustBeWildToSow>
			<sowResearchPrerequisites/>
			<sowTags>
				<li>Ground</li>
			</sowTags>
			<fertilityMin>0.05</fertilityMin>
			<maxMeshCount>25</maxMeshCount>
			<visualSizeRange>0.3~0.4</visualSizeRange>
			<topWindExposure>0.3</topWindExposure>
			<wildClusterRadius>4</wildClusterRadius>
			<wildClusterWeight>8</wildClusterWeight>
			<wildOrder>1</wildOrder>
			<purpose>Beauty</purpose>
		</plant>
	</ThingDef>
