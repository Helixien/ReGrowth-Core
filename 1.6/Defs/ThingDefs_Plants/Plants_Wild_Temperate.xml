	<ThingDef ParentName="TreeBase">
		<defName>RG_Plant_TreeCornish</defName>
		<label>cornish tree</label>
		<description>A large mediterranean conifer covered with tiny pine cones. It grows exclusively in temperate biomes.</description>
		<graphicData>
			<texPath>Things/Plant/RG_TreeCornish</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<shadowData>
				<volume>(0.15, 0.3, 0.15)</volume>
				<offset>(0,0,-0.38)</offset>
			</shadowData>
		</graphicData>
		<statBases>
			<Beauty>3</Beauty>
			<BeautyOutdoors>3</BeautyOutdoors>
		</statBases>
		<ingestible/>
		<plant>
			<growDays>20</growDays>
			<visualSizeRange>1.1~2.6</visualSizeRange>
			<harvestWork>825</harvestWork>
			<harvestYield>25</harvestYield>
