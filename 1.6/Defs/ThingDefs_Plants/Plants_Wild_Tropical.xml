	<!--=============== Tropical Oxalis ==============-->
	<ThingDef ParentName="PlantBase">
		<defName>RG_Plant_Oxalis</defName>
		<label>oxalis</label>
		<description>A small purple flower which grows in large clusters.</description>
		<statBases>
			<MaxHitPoints>85</MaxHitPoints>
			<Nutrition>0.18</Nutrition>
			<Beauty>5</Beauty>
			<BeautyOutdoors>5</BeautyOutdoors>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/RG_TropicalOxalis</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<hideAtSnowOrSandDepth>0.5</hideAtSnowOrSandDepth>
		<ingestible/>
		<plant>
			<fertilitySensitivity>0.0</fertilitySensitivity>
			<growDays>2.5</growDays>
			<mustBeWildToSow>true</mustBeWildToSow>
			<sowResearchPrerequisites/>
			<sowTags>
				<li>Ground</li>
			</sowTags>
			<fertilityMin>0.05</fertilityMin>
			<maxMeshCount>25</maxMeshCount>
			<visualSizeRange>0.3~0.4</visualSizeRange>
			<topWindExposure>0.3</topWindExposure>
			<wildClusterRadius>4</wildClusterRadius>
			<wildClusterWeight>8</wildClusterWeight>
			<wildOrder>1</wildOrder>
			<purpose>Beauty</purpose>
		</plant>
	</ThingDef>
	<!--=============== Plumeria ==============-->
	<ThingDef ParentName="PlantBase">
		<defName>RG_Plant_Plumeria</defName>
		<label>plumeria</label>
		<description>A soft	velvety flower which grows in large clusters.</description>
		<statBases>
			<MaxHitPoints>85</MaxHitPoints>
			<Beauty>4</Beauty>
			<BeautyOutdoors>4</BeautyOutdoors>
			<Nutrition>0.25</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/RG_Plumeria</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<hideAtSnowOrSandDepth>0.5</hideAtSnowOrSandDepth>
		<ingestible/>
		<plant>
			<fertilitySensitivity>0.0</fertilitySensitivity>
			<mustBeWildToSow>true</mustBeWildToSow>
			<sowResearchPrerequisites/>
			<growDays>2.5</growDays>
			<sowTags>
				<li>Ground</li>
			</sowTags>
			<fertilityMin>0.05</fertilityMin>
			<maxMeshCount>25</maxMeshCount>
			<visualSizeRange>0.3~0.4</visualSizeRange>
			<topWindExposure>0.3</topWindExposure>
			<wildClusterRadius>4</wildClusterRadius>
			<wildClusterWeight>8</wildClusterWeight>
			<wildOrder>1</wildOrder>
			<purpose>Beauty</purpose>
		</plant>
	</ThingDef>
	<!--=============== Neo Amanita ==============-->
	<ThingDef ParentName="PlantBase">
		<defName>RG_NeoAmanita</defName>
		<label>neo amanita</label>
		<description>A genetically engineered version of the common amanita to be save for human consumption growing in clusters. It grows exclusively in tropical regions.</description>
		<graphicData>
			<texPath>Things/Plant/RG_NeoAmanita</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<statBases>
			<MaxHitPoints>120</MaxHitPoints>
			<Nutrition>0.4</Nutrition>
			<Beauty>2</Beauty>
			<BeautyOutdoors>2</BeautyOutdoors>
		</statBases>
		<selectable>true</selectable>
		<neverMultiSelect>false</neverMultiSelect>
		<pathCost>15</pathCost>
		<ingestible/>
		<plant>
			<fertilityMin>0.04</fertilityMin>
			<fertilitySensitivity>0.15</fertilitySensitivity>
			<growDays>16</growDays>
			<maxMeshCount>4</maxMeshCount>
			<dieIfLeafless>true</dieIfLeafless>
			<harvestYield>28</harvestYield>
			<harvestTag>Standard</harvestTag>
			<harvestedThingDef>RawFungus</harvestedThingDef>
			<neverBlightable>true</neverBlightable>
			<visualSizeRange>0.5~0.7</visualSizeRange>
			<topWindExposure>0.2</topWindExposure>
			<purpose>Food</purpose>
		</plant>
	</ThingDef>
	<!--=============== Dwarf Palm Tree ==============-->
	<ThingDef ParentName="TreeBase">
		<defName>RG_Plant_TreeDwarfPalm</defName>
		<label>dwarf palm tree</label>
		<description>A short tropical tree with a naked trunk topped by a cluster of very broad leaves.</description>
		<graphicData>
			<texPath>Things/Plant/RG_TreeTinyPalm</texPath>
			<drawSize>1</drawSize>
			<shadowData>
				<volume>(0.18, 0.3, 0.15)</volume>
				<offset>(0.03,0,-0.44)</offset>
			</shadowData>
		</graphicData>
		<statBases>
			<Beauty>1</Beauty>
			<BeautyOutdoors>1</BeautyOutdoors>
		</statBases>
		<plant>
			<growDays>6</growDays>
			<harvestWork>205</harvestWork>
			<harvestYield>5</harvestYield>
			<wildClusterRadius>10</wildClusterRadius>
			<wildClusterWeight>11</wildClusterWeight>
			<topWindExposure>0.15</topWindExposure>
		</plant>
		<ingestible/>
	</ThingDef>
	<!--=============== Tall Palm Tree ==============-->
	<ThingDef ParentName="TreeBase">
		<defName>RG_Plant_TallPalmTree</defName>
		<label>palm tree</label>
		<description>A tropical tree with a long naked trunk topped by a cluster of very broad leaves. Unfortunately, this variety doesn't produce any edible coconuts.</description>
		<graphicData>
			<texPath>Things/Plant/RG_TreeTallPalm</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<shadowData>
				<volume>(0.05, 0.6, 0.15)</volume>
				<offset>(0,0,-0.25)</offset>
			</shadowData>
		</graphicData>
		<statBases>
			<Beauty>2</Beauty>
			<BeautyOutdoors>2</BeautyOutdoors>
		</statBases>
		<plant>
			<growDays>48</growDays>
			<visualSizeRange>2.0~3.0</visualSizeRange>
			<harvestWork>1100</harvestWork>
			<harvestYield>32</harvestYield>
			<immatureGraphicPath>Things/Plant/_Immature/RG_TreePalm_Immature</immatureGraphicPath>
		</plant>
	</ThingDef>
</Defs>