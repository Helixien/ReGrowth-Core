<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!--=============== Tropical Brambles ==============-->
	<ThingDef ParentName="RG_BramblesBase">
		<defName>RG_Plant_TropicalBrambles</defName>
		<description>Tangled, thorny, woody shoots covered with leaves. Brambles grow in clusters, and slow down anyone moving over them.</description>
		<graphicData>
			<texPath>Things/Plant/RG_TropicalBrambles</texPath>
		</graphicData>
	</ThingDef>
	<!--=============== Tropical Ivy ==============-->
	<ThingDef ParentName="PlantBase">
		<defName>RG_Plant_TropicalIvy</defName>
		<label>ivy</label>
		<description>Wild ivy with leafs that grows in patches of leaves in areas of tropical temperatures.</description>
		<statBases>
			<MaxHitPoints>120</MaxHitPoints>
			<Flammability>0.6</Flammability>
			<Nutrition>0.15</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/RG_TropicalIvy</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<hideAtSnowOrSandDepth>0.5</hideAtSnowOrSandDepth>
		<ingestible/>
		<plant>
			<fertilityMin>0.05</fertilityMin>
			<fertilitySensitivity>0.2</fertilitySensitivity>
			<growDays>15</growDays>
			<leaflessGraphicPath>Things/Plant/_Leafless/RG_TropicalIvy_Leafless</leaflessGraphicPath>
			<harvestWork>75</harvestWork>
			<maxMeshCount>4</maxMeshCount>
			<wildClusterRadius>4</wildClusterRadius>
			<wildClusterWeight>8</wildClusterWeight>
			<visualSizeRange>0.7~1.2</visualSizeRange>
			<topWindExposure>0.2</topWindExposure>
			<wildOrder>1</wildOrder>
		</plant>
	</ThingDef>
	<!--=============== Tropical Fern ==============-->
	<ThingDef ParentName="BushBase">
		<defName>RG_Plant_TropicalFern</defName>
		<label>fern</label>
		<description>A short shrub with long, fine leaves. Grows in tropical areas.</description>
		<graphicData>
			<texPath>Things/Plant/RG_TropicalFern</texPath>
			<drawSize>1.15</drawSize>
			<shadowData>
				<volume>(0.55, 0.2, 0.2)</volume>
			</shadowData>
		</graphicData>
		<uiIconScale>0.85</uiIconScale>
		<plant>
			<growDays>3</growDays>
			<leaflessGraphicPath>Things/Plant/_Leafless/RG_TropicalFern_Leafless</leaflessGraphicPath>
			<harvestWork>75</harvestWork>
		</plant>
	</ThingDef>
	<!--=============== Tropical Chokevine ==============-->
	<ThingDef ParentName="PlantBase">
		<defName>RG_Plant_TropicalChokevine</defName>
		<label>chokevine</label>
		<description>Tangled masses of cord-like, spiky vines which coil in great masses along the ground. Chokevine greatly slows down anyone who moves over it. Multiple tribes have independently generated stories of chokevine strangling children to death in the night, as a way to scare kids into coming home before dark.</description>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<Nutrition>0.25</Nutrition>
		</statBases>
		<pathCost>42</pathCost>
		<graphicData>
			<texPath>Things/Plant/RG_TropicalChokevine</texPath>
			<drawSize>2.25</drawSize>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<ingestible/>
		<plant>
			<fertilitySensitivity>0.7</fertilitySensitivity>
			<growDays>5</growDays>
			<harvestWork>150</harvestWork>
			<maxMeshCount>4</maxMeshCount>
			<leaflessGraphicPath>Things/Plant/_Leafless/RG_Chokevine_Leafless</leaflessGraphicPath>
			<pollutedGraphicPath>Things/Plant/_Polluted/RG_Chokevine_Polluted</pollutedGraphicPath>
			<visualSizeRange>0.78~0.88</visualSizeRange>
			<topWindExposure>0.16</topWindExposure>
			<wildClusterRadius>6</wildClusterRadius>
			<wildClusterWeight>200</wildClusterWeight>
			<wildOrder>1</wildOrder>
			<wildEqualLocalDistribution>false</wildEqualLocalDistribution>
			<pollution>Any</pollution>
		</plant>
	</ThingDef>
	<!--=============== Tidalis ==============-->
	<ThingDef ParentName="PlantBase">
		<defName>RG_Plant_Tidalis</defName>
		<label>tidalis</label>
		<description>A genetically altered version of common ivy crossed with a tropical flower adapted to thrive in tropical areas.</description>
		<graphicData>
			<texPath>Things/Plant/RG_Tidalis</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<drawSize>1.5</drawSize>
		</graphicData>
		<statBases>
			<MaxHitPoints>120</MaxHitPoints>
			<Flammability>0.6</Flammability>
			<Nutrition>0.15</Nutrition>
			<Beauty>3</Beauty>
			<BeautyOutdoors>3</BeautyOutdoors>
		</statBases>
		<hideAtSnowOrSandDepth>0.5</hideAtSnowOrSandDepth>
		<ingestible/>
		<plant>
			<fertilityMin>0.05</fertilityMin>
			<fertilitySensitivity>0.2</fertilitySensitivity>
			<growDays>15</growDays>
			<harvestWork>75</harvestWork>
			<maxMeshCount>4</maxMeshCount>
			<wildClusterRadius>4</wildClusterRadius>
			<wildClusterWeight>4</wildClusterWeight>
			<visualSizeRange>0.25~0.75</visualSizeRange>
			<topWindExposure>0.25</topWindExposure>
			<wildOrder>1</wildOrder>
		</plant>
	</ThingDef>
