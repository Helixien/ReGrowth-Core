<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationSequence">
		<!-- ================================ Plant Patches	====================== -->
		<operations>
			<!-- ================================ HD Nutrifungus ====================== -->
			<li Class="PatchOperationReplace">
				<xpath>/Defs/ThingDef[defName="Plant_Nutrifungus"]/graphicData/texPath</xpath>
				<value>
					<texPath>Things/Plant/RG_Nutrifungus</texPath>
