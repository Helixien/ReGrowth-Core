<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationSequence">
		<!-- ================================ Plant Texture Patches - Trees	====================== -->
		<operations>
			<!-- ================================ HD Willowgill ====================== -->
			<li Class="PatchOperationReplace">
				<xpath>/Defs/ThingDef[defName="Plant_Willowgill"]/graphicData/texPath</xpath>
				<value>
					<texPath>Things/Plant/RG_Willowgill</texPath>
				</value>
			</li>
			<!-- ================================ HD Boomshroom ====================== -->
			<li Class="PatchOperationReplace">
				<xpath>/Defs/ThingDef[defName="Boomshroom"]/graphicData/texPath</xpath>
				<value>
					<texPath>Things/Plant/RG_Boomshroom</texPath>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ThingDef[defName="Boomshroom"]/plant</xpath>
				<value>
					<immatureGraphicPath>Things/Plant/_Immature/RG_Boomshroom_Immature</immatureGraphicPath>
				</value>
			</li>
		</operations>
	</Operation>
