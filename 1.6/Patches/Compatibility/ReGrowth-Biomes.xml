<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<!--============================================== ReGrowth: Aspen Forests ==============================================-->
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>ReGrowth: Aspen</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<!-- ================ Biome Apsen Forest ================ -->
				<li Class="PatchOperationAdd">
					<xpath>/Defs/BiomeDef[defName = "RG_AspenForest"]/wildPlants</xpath>
					<value>
						<RG_Plant_Raspberry>0.03</RG_Plant_Raspberry>
						<RG_Plant_BorealFern>0.2</RG_Plant_BorealFern>
						<RG_Tree_TundraTreePine>0.25</RG_Tree_TundraTreePine>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>