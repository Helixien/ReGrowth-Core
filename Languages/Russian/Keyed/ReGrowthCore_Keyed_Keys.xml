  <!-- Enable aspen leaf spawning. -->
  <RG.SpawnLeaves>Включить появление осиновой листовой грязи.</RG.SpawnLeaves>
  <!-- Cold fog -->
  <RG.ColdFog>Холодный туман</RG.ColdFog>
  <!-- Ice layer -->
  <RG.IceLayer>Слой льда</RG.IceLayer>
  <!-- Rain water puddles -->
  <RG.RainWaterPuddles>Лужи от дождя</RG.RainWaterPuddles>
  <!-- Clean water puddles -->
  <RG.RainCleanWaterPuddles>Убрать лужи от дождя</RG.RainCleanWaterPuddles>
  <!-- Fog velocity -->
  <RG.FogVelocity>Скорость тумана</RG.FogVelocity>
  <!-- Fog temperature -->
  <RG.FogTemp>Температура тумана</RG.FogTemp>
